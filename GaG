repeat task.wait() until game:IsLoaded()
if _G.Thai == true then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostSobe/Script/refs/heads/main/GrowaGardenThai"))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostSobe/Script/refs/heads/main/GrowaGraden"))()
end
